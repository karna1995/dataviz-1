                <button class="filterType btn btn-lg btn-primary"># Clear Filter</button>
            </div>
      </div>
      <div class="modal-footer">
      </div>
    </div>
  </div>
</div>

<div class="modal fade filterNumeric" data-backdrop="static"  data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Filter Numbers</h4>
      </div>
      <div class="modal-body" style="">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="range active"><a href="#tabRange" role="tab" data-toggle="tab">Range</a></li>
                <li role="presentation" class="gte"><a href="#tabGTE" role="tab" data-toggle="tab">At Least</a></li>
                <li role="presentation" class="lte"><a href="#tabLTE" role="tab" data-toggle="tab">At Most</a></li>
                <li class="hidden" role="presentation"><a href="#tabSpecial" role="tab" data-toggle="tab">Special</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="tabRange">
                    <div class="panel-body">
                        <label class="mark rangeValue"></label>
                        <div class="rangeSlider"></div>
                        <div><br>
                        <label><input type="checkbox">&nbsp;Include Null Values</label>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="tabGTE">
                    <div class="panel-body">
                        <label class="mark gteValue"></label>
                        <div class="gteSlider"></div>
                        <div><br>
                        <label><input type="checkbox">&nbsp;Include Null Values</label>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="tabLTE">
                    <div class="panel-body">
                        <label class="mark lteValue"></label>
                        <div class="lteSlider"></div>
                        <div><br>
                        <label><input type="checkbox">&nbsp;Include Null Values</label>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="hidden tab-pane" id="tabSpecial">
                </div>
            </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary applyButton">Apply</button>
        <button type="button" class="btn btn-default clearButton" data-dismiss="modal">Clear</button>
      </div>
    </div>
  </div>
</div>


<div class="modal fade filterString" data-backdrop="static"  data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="hidden close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title">Filter Strings</h4>
      </div>
      <div class="modal-body" style="">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li class="general active" role="presentation"><a href="#tabGeneral" role="tab" data-toggle="tab">General</a></li>
                <li class="wildcard" role="presentation"><a href="#tabWildcard" role="tab" data-toggle="tab">Wildcard</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="tabGeneral">
                    <div class="panel-body">
                        <input class="hidden form-control" placeholder="Enter search text">
                        <div class="stringCheckBoxes" style="max-height: 200px;overflow-y: scroll;">
                            <div><label><input type="checkbox"> Foo</label></div>
                            <div><label><input type="checkbox"> Bar</label></div>
                            <div><label><input type="checkbox"> Baz</label></div>
                            <div><label><input type="checkbox"> Foo</label></div>
                            <div><label><input type="checkbox"> Bar</label></div>
                            <div><label><input type="checkbox"> Baz</label></div>
                            <div><label><input type="checkbox"> Foo</label></div>
                            <div><label><input type="checkbox"> Bar</label></div>
                            <div><label><input type="checkbox"> Baz</label></div>
                            <div><label><input type="checkbox"> Foo</label></div>
                            <div><label><input type="checkbox"> Bar</label></div>
                            <div><label><input type="checkbox"> Baz</label></div>
                        </div>
                        
                        <div>
                            <button onclick="$('.stringCheckBoxes input[type=&quot;checkbox&quot;]').prop('checked',true)" type="button" class="btn btn-xs btn-default">All</button>
                            <button onclick="$('.stringCheckBoxes input[type=&quot;checkbox&quot;]').prop('checked',false)" type="button" class="btn btn-xs btn-default">None</button>
                            <label class="pull-right">Exclude <input id="generalExclude"  class="" type="checkbox"></label>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="tabWildcard">
                      <div class="panel-body" >
                          <div style="min-height: 200px; max-height: 200px;overflow-y: scroll;">
                            <label>Match Value:</label>
                            <label class="pull-right">Exclude <input id="WcExclude" type="checkbox"> </label>
                            <input type="text" id="txtWcValue" >
                            <div class="input-group">
                                <div><input checked name="WcType" value="contains" type="radio">&nbsp;Contains</div>
                                <div><input name="WcType" value="startswith" type="radio">&nbsp;Starts with</div>
                                <div><input name="WcType" value="endswith" type="radio">&nbsp;Ends with</div>
                                <div><input name="WcType" value="equals" type="radio">&nbsp;Exactly matches</div>
                            </div>
                          </div>
                          <label class=""><input id="WcIncludeAll" checked type="checkbox">&nbsp;Include all values when empty</label>
                      </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="tabLTE"></div>
                <div role="tabpanel" class="tab-pane" id="tabSpecial"></div>
            </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default btn-primary applyButton" >Apply</button>
        <button type="button" class="btn btn-default clearButton" data-dismiss="modal">Clear</button>
      </div>
    </div>
  </div>
</div>

